    "bootstrap": "4.6.0",
    "classnames": "^2.3.1",
    "formik": "^2.2.9",
    "node-sass": "^6.0.1",
    "node-sass-package-importer": "^5.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-charts": "^3.0.15",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "reactstrap": "^8.9.0",
